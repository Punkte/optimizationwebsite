{
  "routes": [
    {
      "src": "/assets/images/(.*)",
      "headers": {
        "cache-control": "max-age=0, s-maxage=86400, public"
      },
      "dest": "/images/$1"
    },
    {
      "src": "/assets/(.*).(js|css)",
      "headers": {
        "Accept-Encoding": "gzip",
        "cache-control": "max-age=0, s-maxage=86400, public"
      },
      "dest": "/assets/$1"
    }
  ]
}