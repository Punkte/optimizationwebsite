{
  "routes": [
    {
      "src": "/assets/images/(.*)",
      "headers": {
        "Accept-Encoding": "gzip",
        "cache-control": "s-maxage=604800",
        "ExpiresDefault": "access plus 7200 seconds"
      },
      "dest": "/images/$1"
    },
    {
      "src": "/assets/(.*).(js|css)",
      "headers": {
        "Accept-Encoding": "gzip",
        "cache-control": "s-maxage=604800",
        "ExpiresDefault": "access plus 7200 seconds"
      },
      "dest": "/assets/$1"
    }
  ]
}